fn main(argc, argv) {
    print("Hello from LatexTemplate!");
    print("Nombre d'arguments:", argc);
    print("Premier argument:", argv[0]);
    
    let x = 42;
    let result = x + 8;
    print("Calcul: 42 + 8 =", result);
    
    for i in 1..5 {
        print("Compteur:", i);
    }
    
    return <% template %>
        Bonjour ! Ceci est un template généré.
        Le calcul donne: <% result %>
        
        <% for j in 1..3 %>
            Ligne <% j %>
             
        <% endfor %>
    <% endtemplate %>;
} 